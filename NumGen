#!/bin/bash

# Number Generator

echo :: NumGen ::
echo 
echo Generating A: 1 MiB and B: 1 MiB numbers...

NUM_FILE=MyNum

rm $NUM_FILE
touch $NUM_FILE

COUNTER=0

while [ $COUNTER -lt 100 ]
do
	echo -n  >> $NUM_FILE
	let COUNTER=COUNTER+1 
done

echo : >> $NUM_FILE

COUNTER=0

while [ $COUNTER -lt 100 ]
do
	echo -n  >> $NUM_FILE
	let COUNTER=COUNTER+1 
done

echo 
echo --- Done ---

# End
